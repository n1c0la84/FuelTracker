package net.nicolanicodemo.fueltracker.model;

import java.io.Serializable;

public class Rifornimento implements Serializable {
	private static final long serialVersionUID = 1L;
	
	private int ID;
	private String data;
	private double spesa;
	private double litri;
	private int kilometri;
	private double costoUnitario;
	private boolean serbatoioPieno;
	private double consumo100;
	
	public int getID(){
		return ID;
	}

	public String getData() {
		return data;
	}

	public double getSpesa() {
		return spesa;
	}

	public double getLitri() {
		return litri;
	}

	public int getKilometri() {
		return kilometri;
	}

	public double getCostoUnitario() {
		return costoUnitario;
	}

	public boolean isSerbatoioPieno() {
		return serbatoioPieno;
	}

	public double getConsumo100() {
		return consumo100;
	}

	public void setID(int ID) {
		this.ID = ID;
	}

	public void setData(String data) {
		this.data = data;
	}

	public void setSpesa(double spesa) {
		this.spesa = spesa;
	}

	public void setLitri(double litri) {
		this.litri = litri;
	}
	
	public void setKilometri(int kilometri) {
		this.kilometri = kilometri;
	}

	public void setCostoUnitario(double costoUnitario) {
		this.costoUnitario = costoUnitario;
	}
	
	public void setSerbatoioPieno(boolean serbatoioPieno) {
		this.serbatoioPieno = serbatoioPieno;
	}
	
	public void setConsumo100(double consumo100) {
		this.consumo100 = consumo100;
	}
	
}
